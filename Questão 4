#include <stdio.h>

int somaentre2inteiros(int numeromenorLocal, int numeromaiorLocal);

int main()
{
	int numero1, numero2, numeromaior, numeromenor, soma = 0; 
	printf("digite o primeiro numero\n");
	scanf("%d", &numero1);
	printf("digite o segundo numero\n");
	scanf("%d", &numero2);
	if (numero1 > numero2){
		numeromaior = numero1;
		numeromenor = numero2;
	}else{
		numeromaior = numero2;
		numeromenor = numero1;
	}
	somaentre2inteiros(numeromenor, numeromaior);
	for(numeromenor += 1;numeromenor < numeromaior; numeromenor++){
		soma += numeromenor;
	}
	printf("\nsoma entre os dois numeros = %d", soma);
	return 0;
}
int somaentre2inteiros(int numeromenorLocal, int numeromaiorLocal){
	int somaLocal;
	for(numeromenorLocal += 1;numeromenorLocal < numeromaiorLocal; numeromenorLocal++){
		somaLocal += numeromenorLocal;
	}
	return somaLocal;
}
