#include <stdio.h>
int main(void)
{
	int Quantiadedovetor, i;
	scanf("%d", &Quantiadedovetor);
	int vetor[Quantiadedovetor], novovetor[Quantiadedovetor];
	for(i = 0; i < Quantiadedovetor; i++){
		vetor[i] = i;
		printf("%d\n\n", vetor[i]);
	}
	for (i = 0; i < Quantiadedovetor; i ++){
		novovetor[i] = vetor[(Quantiadedovetor - i) - 1];
		printf("ao contrario %d", novovetor[i]);
		printf("         %d            %d\n", i, Quantiadedovetor);
	}
	return 0;
}
